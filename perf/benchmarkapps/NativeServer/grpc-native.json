{
  "Default": {
    "Client": "Grpc",
    "ClientProperties": {
      "UseTls": "false"
    },

    "Source": {
      "Repository": "https://github.com/jamesnk/grpc-dotnet.git",
      "BranchOrCommit": "jamesnk/benchmarks-h2load",
      "Project": "perf/benchmarkapps/NativeServer/NativeServer.csproj"
    },
    "Port": 50051
  },

  "GrpcUnaryNative-h2load": {
    "Client": "h2load",
    "Path": "/Greet.Greeter/SayHello",
    "Headers": {
      "content-type": "application/grpc"
    },
    "ClientProperties": {
      "protocol": "h2c",
      "RequestBody": "AAAAAAcKBVdvcmxk"
    }
  },

  "GrpcUnaryNative": {
    "ClientProperties": {
      "Scenario": "Unary"
    }
  }
}
